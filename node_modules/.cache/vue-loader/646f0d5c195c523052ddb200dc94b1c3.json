f } from 'vue';\nexport default defineComponent({\n  props: {\n    nft: { type: Object, required: true },\n  },\n  emits: ['selected'],\n  setup(props, ctx) {\n    const selected = ref<boolean>(false);\n\n    const toggleSelect = () => {\n      selected.value = !selected.value;\n      console.log('selected', props.nft.mint.toBase58());\n\n      // console.log('newselected.............', {\n      //   nft: props.nft,\n      //   selected: selected.value,\n      // });\n      ctx.emit('selected', {\n        nft: props.nft,\n        selected: selected.value,\n      });\n    };\n\n    return {\n      selected,\n      toggleSelect,\n    };\n  },\n});\n</script>\n\n<style scoped>\nimg {\n  max-width: 100%;\n  max-height: 100%;\n  height: auto;\n  width: auto;\n}\n\n.card {\n  width: 150px;\n  height: 150px;\n}\n\n.card:hover {\n  @apply border-4 border-solid border-gray-200;\n}\n\n.card-selected {\n  @apply border-4 border-solid;\n  border-color: palegreen !important;\n}\n</style>\n"]}]}